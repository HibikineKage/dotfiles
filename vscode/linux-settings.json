{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "mrmlnc.vscode-remark"
  },
  "[jade]": {
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[review]": {
    "editor.insertSpaces": false
  },
  "breadcrumbs.enabled": true,
  "editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "Ricty Diminished, Consolas, 'Courier New', monospace, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/tests/_ci_phpunit_test/tmp/**": true,
    "**/.factorypath": true
  },
  "files.associations": {
    ".remarkrc": "json"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/tests/_ci_phpunit_test/tmp/**": true,
    "**/node_modules/*/**": true
  },
  "files.insertFinalNewline": true,
  "window.zoomLevel": 0,
  "workbench.editor.enablePreview": false,
  "eslint.packageManager": "yarn",
  "circleci.apiKey": "099d7f5848373c792a0f25bc0f53b71520a0ad2c",
  "csharp.format.enable": false,
  "FSharp.monoPath": "/usr/bin/mono",
  "FSharp.fsacRuntime": "net",
  "php-cs-fixer.autoFixBySemicolon": true,
  "php-cs-fixer.allowRisky": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.requireConfig": false,
  "prettier.disableLanguages": ["markdown"],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "msbuildProjectTools": {
    "logging": {
      "level": "Information",
      "seq": {},
      "file": "",
      "trace": false
    },
    "language": {
      "disableFeature": {},
      "useClassicProvider": false,
      "disable": {
        "hover": false
      }
    },
    "nuget": {
      "newestVersionsFirst": true,
      "includePreRelease": true,
      "disablePrefetch": false
    },
    "schemaVersion": 1
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "html.format.endWithNewline": true,
  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "typescript.npm": "/usr/bin/npm",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "auto-rename-tag.activationOnLanguage": [
    "javascriptreact",
    "html",
    "vue",
    "xml",
    "typescriptreact"
  ],
"vim.autoSwitchInputMethod.obtainIMCmd": "/usr/bin/fcitx-remote",
"vim.autoSwitchInputMethod.switchIMCmd": "/usr/bin/fcitx-remote {im}",
"vim.autoSwitchInputMethod.defaultIM": "-c",
"php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",

"typescript.preferences.importModuleSpecifier": "relative",
"editor.tokenColorCustomizations": {
    "semanticHighlighting": true
},
"diffEditor.ignoreTrimWhitespace": false,
"vim.camelCaseMotion.enable": true,
"cSpell.userWords": [
  "Fursuit"
],
}
